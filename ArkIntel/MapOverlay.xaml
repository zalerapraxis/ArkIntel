<Window x:Class="ArkIntel.MapOverlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArkIntel"
        mc:Ignorable="d"
        Title="Map Overlay" Height="630" Width="600" ResizeMode="NoResize" Closed="Window_Closed">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Image Grid.Row="1" Grid.ZIndex="1" Source="volcano_map.jpg"/>
        <Canvas x:Name="canvasDinoPositions" Grid.Row="1" Grid.ZIndex="2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500" Height="500"/>
        <Canvas x:Name="canvasDinoPositionsHighlighted" Grid.Row="1" Grid.ZIndex="3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500" Height="500"/>
    </Grid>
</Window>
